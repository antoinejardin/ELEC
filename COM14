help egen
egen vg = rowtotal (g1-g32)
egen vd = rowtotal (d1-d32)
egen vd = rowtotal (d2-d32)
egen vfn = rowtotal (fn2-fn32)
gen vo = exprims - ( vg + vd + vfn)
list vg vd vfn vo
gen gexp = ( vg / exprims ) * 100
gen dexp = ( vd / exprims ) * 100
gen fnexp = ( vfn / exprims ) * 100
gen gins= ( vg / inscrits ) * 100
gen dins = ( vd / inscrits ) * 100
gen fnins = ( vfn / inscrits ) * 100
